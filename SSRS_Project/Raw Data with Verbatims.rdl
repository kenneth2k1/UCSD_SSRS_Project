				</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="SurveyID"><DataField>SurveyID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="SurveyTitle"><DataField>SurveyTitle</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="SurveyTypeID"><DataField>SurveyTypeID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="SurveyYear"><DataField>SurveyYear</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="OverallRspCount"><DataField>OverallRspCount</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="SurveyYearTitle"><DataField>SurveyYearTitle</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="OverallRspCompleteCount"><DataField>OverallRspCompleteCount</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="SurveySCDOriginalID"><DataField>SurveySCDOriginalID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="AuditKey"><DataField>AuditKey</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="dsQuestionCategories"><Query><DataSourceName>SurveyDW</DataSourceName><QueryParameters><QueryParameter Name="@SurveyID"><Value>=Parameters!SurveyId.Value</Value><rd:UserDefined xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UserDefined></QueryParameter></QueryParameters><CommandText>
					SELECT DISTINCT qc.QuestionCategoryID, qc.QuestionCategoryLabel, qc.SurveyTypeName, qc.QuestionCategorySCDOriginalID, qc.AuditKey, 'OrderBy' = qc.QuestionCategoryLabel
					FROM            DW_DimQuestionCategory AS qc INNER JOIN
					DW_FactQuestionAnswerCategories AS f ON qc.QuestionCategoryID = f.QuestionCategoryID
					WHERE        (f.QuestionAnsID IN
					(SELECT        QuestionAnsID
					FROM            DW_FactAnswer
					WHERE        (SurveyID = @SurveyID)))
					union

					SELECT DISTINCT 'QuestionCategoryID' = -1, 'QuestionCategoryLabel'= 'Not Specified', 'SurveyTypeName'='NA', 'QuestionCategorySCDOriginalID'=-1, 'AuditKey'='-1', 'OrderBy' = 'AAAAAA'

					ORDER BY OrderBy
				</CommandText></Query><Fields><Field Name="QuestionCategoryID"><DataField>QuestionCategoryID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="QuestionCategoryLabel"><DataField>QuestionCategoryLabel</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="SurveyTypeName"><DataField>SurveyTypeName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="QuestionCategorySCDOriginalID"><DataField>QuestionCategorySCDOriginalID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="AuditKey"><DataField>AuditKey</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="OrderBy"><DataField>OrderBy</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="dsResponseStatuses"><Query><DataSourceName>SurveyDW</DataSourceName><CommandText>select * from DW_DimResponseStatus</CommandText></Query><Fields><Field Name="StatusID"><DataField>StatusID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="StatusName"><DataField>StatusName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="StatusSCDOriginalID"><DataField>StatusSCDOriginalID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="AuditKey"><DataField>AuditKey</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="dsGroups"><Query><DataSourceName>SurveyDW</DataSourceName><QueryParameters><QueryParameter Name="@SurveyID"><Value>=Parameters!SurveyId.Value</Value></QueryParameter></QueryParameters><CommandText>
					DECLARE @pad char(10); SET @pad = '0000000000'

					select distinct g.GroupID, g.GroupCode, g.GroupName, g.SurveyTypeName, g.Path, g.Depth, 'GroupTitle' = cast(g.GroupCode as varchar) + ' - ' + g.GroupName
					from DW_DimGroup g
					join DW_DimSurveyType st on (g.SurveyTypeId = st.SurveyTypeId)
					join (
					select 'SurveyPathOnly' =  right(@pad + convert(varchar, s.SurveyTypeID), len(@pad)) + '/'
					from DW_DimSurveyType st
					join DW_DimSurvey s on (st.SurveyTypeID = s.SurveyTypeID)
					where s.SurveyID in (@SurveyID)
					) sPath on (st.Path LIKE '%' + sPath.SurveyPathOnly + '%')
					where g.IsArchived = 0

					union

					select distinct 'GroupID' = -1, 'GroupCode'=-1, 'GroupName' = '- Not Specified -', 'SurveyTypeName' = 'AAAAAA', 'Path' = '0', 'Depth' = 0, 'GroupTitle' = '- Not Specified -'
					order by g.SurveyTypeName, g.Path, g.Depth, g.GroupName
				</CommandText></Query><Fields><Field Name="GroupID"><DataField>GroupID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="GroupCode"><DataField>GroupCode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="GroupName"><DataField>GroupName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="SurveyTypeName"><DataField>SurveyTypeName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Path"><DataField>Path</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Depth"><DataField>Depth</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="GroupTitle"><DataField>GroupTitle</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="GroupTitleIndented"><Value>=Space(3*Fields!Depth.Value) + Fields!GroupTitle.Value</Value></Field></Fields></DataSet><DataSet Name="dsSecondaryQuestionCategories"><Query><DataSourceName>SurveyDW</DataSourceName><QueryParameters><QueryParameter Name="@SurveyID"><Value>=Parameters!SurveyId.Value</Value></QueryParameter><QueryParameter Name="@QuestionCategoryIds"><Value>=Parameters!QuestionCategoryIds.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT DISTINCT qc.QuestionCategoryID, qc.QuestionCategoryLabel, qc.SurveyTypeName, qc.QuestionCategorySCDOriginalID, qc.AuditKey, 'OrderBy' = qc.QuestionCategoryLabel
FROM DW_DimQuestionCategory AS qc 
	INNER JOIN DW_FactQuestionAnswerCategories AS f ON qc.QuestionCategoryID = f.QuestionCategoryID
WHERE f.QuestionAnsID IN (SELECT QuestionAnsID FROM DW_FactAnswer WHERE SurveyID = @SurveyID) and qc.QuestionCategoryID not in (@QuestionCategoryIds)

union

