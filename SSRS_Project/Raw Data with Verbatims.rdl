/Value></Values></DefaultValue><Prompt>Categories: </Prompt><ValidValues><DataSetReference><DataSetName>dsQuestionCategories</DataSetName><ValueField>QuestionCategoryID</ValueField><LabelField>QuestionCategoryLabel</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter><ReportParameter Name="SecondaryQuestionCategoryIds"><DataType>String</DataType><DefaultValue><Values><Value>-1</Value></Values></DefaultValue><Prompt>Secondary Categories: </Prompt><ValidValues><DataSetReference><DataSetName>dsSecondaryQuestionCategories</DataSetName><ValueField>QuestionCategoryID</ValueField><LabelField>QuestionCategoryLabel</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter><ReportParameter Name="RspStatusIds"><DataType>Integer</DataType><DefaultValue><DataSetReference><DataSetName>dsResponseStatuses</DataSetName><ValueField>StatusID</ValueField></DataSetReference></DefaultValue><Prompt>Response Status:</Prompt><ValidValues><DataSetReference><DataSetName>dsResponseStatuses</DataSetName><ValueField>StatusID</ValueField><LabelField>StatusName</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter></ReportParameters><Code>
		public function ConvertToInteger(v as object) as integer
		if (not isnumeric(v)) then
		if (trim(cstr(v)) = "na") then
		return 9
		else
		return 0
		end if
		else
		return cint(v)
		end if
		end function

		public function GetGroups (groups() as string) as string
		dim s as string = ""
		dim ret as string = ""

		for each s in groups
		ret = ret &amp; s &amp; vbcrlf
		next s

		return ret
		end function

		public function GetSurveys (surveys() as string) as string
		dim s as string = ""
		dim ret as string = ""

		for each s in surveys
		ret = ret &amp; s &amp; vbcrlf
		next s

		return ret
		end function

		public function GetQuestionCategories (cats() as string) as string
		dim s as string = ""
		dim ret as string = ""

		for each s in cats
		ret = ret &amp; s &amp; vbcrlf
		next s

		return ret
		end function
