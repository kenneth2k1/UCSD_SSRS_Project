mbers><DataElementOutput>Output</DataElementOutput><KeepTogether>true</KeepTogether></TablixMember></TablixMembers><DataElementOutput>Output</DataElementOutput><KeepTogether>true</KeepTogether></TablixMember></TablixMembers></TablixMember></TablixMembers></TablixMember></TablixMembers></TablixMember></TablixMembers></TablixMember></TablixMembers><DataElementOutput>Output</DataElementOutput></TablixMember></TablixMembers></TablixRowHierarchy><RepeatColumnHeaders>true</RepeatColumnHeaders><RepeatRowHeaders>true</RepeatRowHeaders><DataSetName>dsRawData</DataSetName><Height>0.5in</Height><Width>28.5624in</Width><Style /></Tablix></ReportItems><PageBreak><BreakLocation>Start</BreakLocation></PageBreak><DataElementOutput>ContentsOnly</DataElementOutput><Height>0.5in</Height><Width>28.5624in</Width><Style /></Rectangle></ReportItems><Height>0.5in</Height><Style /></Body><Width>28.5624in</Width><Page><PageHeight>8.5in</PageHeight><PageWidth>11in</PageWidth><InteractiveHeight>11in</InteractiveHeight><InteractiveWidth>8.5in</InteractiveWidth><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="SurveyDW"><DataSourceReference>SurveyDW</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">8e143069-e866-4c80-9b03-ab47ae619999</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="dsRawData"><Query><DataSourceName>SurveyDW</DataSourceName><QueryParameters><QueryParameter Name="@SurveyId"><Value>=Parameters!SurveyId.Value</Value></QueryParameter><QueryParameter Name="@QuestionCategoryIds"><Value>=Parameters!QuestionCategoryIds.Value</Value></QueryParameter><QueryParameter Name="@GroupIds"><Value>=Parameters!GroupIds.Value</Value></QueryParameter><QueryParameter Name="@SecondaryQuestionCategoryIds"><Value>=Parameters!SecondaryQuestionCategoryIds.Value</Value></QueryParameter><QueryParameter Name="@RspStatusIds"><Value>=Parameters!RspStatusIds.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @pad char(10); SET @pad = '0000000000'

-- 1. declare two tables, one for primary categories and the other for related categories
DECLARE @primaryAnswers table (
	RspId int, SurveyId int, GroupId int, QuestionAnsID int, QuestionText varchar(8000), QuestionReportAbbr varchar(255), QuestionAnsDataType varchar(50), 
	AllowsMultipleAnswers bit, AnsLabel varchar(255), QuestionPageNumber int, QuestionDisplayOrder int, QuestionNumber int, DisplayOrder int, 
	QstExternalBusinessKey varchar(1000), AnsText varchar(8000), AnsValue varchar(8000), QuestionCategoryLabel varchar(255)
)

DECLARE @secondaryAnswers table (
	RspId int, SurveyId int, GroupId int, QuestionAnsID int, QuestionText varchar(8000), QuestionReportAbbr varchar(255), QuestionAnsDataType varchar(50), 
	AllowsMultipleAnswers bit, AnsLabel varchar(255), QuestionPageNumber int, QuestionDisplayOrder int, QuestionNumber int, DisplayOrder int, 
	QstExternalBusinessKey varchar(1000), AnsText varchar(8000), AnsValue varchar(8000), QuestionCategoryLabel varchar(255)
)


-- 2. insert the answers related to the PRIMARY categories selected
insert into @primaryAnswers
select fa.RspId, fa.SurveyID, fa.GroupID, qa.QuestionAnsID, qa.QuestionText, qa.QuestionReportAbbr, qa.QuestionAnsDataType, qa.AllowsMultipleAnswers, qa.AnsLabel,
qa.QuestionPageNumber, qa.QuestionDisplayOrder, qa.QuestionNumber, qa.DisplayOrder,
qa.ExternalBusinessKey AS 'QstExternalBusinessKey', fa.AnsText, qa.AnsValue,
'QuestionCategoryLabel'= isnull(fdqc.QuestionCategoryLabel, 'Not Specified')
from DW_FactAnswer AS fa
	LEFT OUTER JOIN DW_DimQuestionAnswer AS qa ON fa.QuestionAnsID = qa.QuestionAnsID
		LEFT OUTER JOIN (
			select fqac.QuestionAnsID, 'QuestionCategoryID' = max(fqac.QuestionCategoryID), 'QuestionCategoryLabel' = max(qc.QuestionCategoryLabel)
			from DW_FactQuestionAnswerCategories fqac
			join DW_DimQuestionCategory qc on (fqac.QuestionCategoryID = qc.QuestionCategoryID)
			group by fqac.QuestionAnsID
			having count(*) = 1
		) AS fdqc ON qa.QuestionAnsID = fdqc.QuestionAnsID
WHERE (fa.SurveyID = @SurveyId) 
	AND (-1 in (@QuestionCategoryIds) or fdqc.QuestionCategoryID IN (@QuestionCategoryIds))

insert into @primaryAnswers
select fa.RspId, fa.SurveyID, fa.GroupID, qa.QuestionAnsID, qa.QuestionText, qa.QuestionReportAbbr, qa.QuestionAnsDataType, qa.AllowsMultipleAnswers, qa.AnsLabel,
qa.QuestionPageNumber, qa.QuestionDisplayOrder, qa.QuestionNumber, qa.DisplayOrder,
qa.ExternalBusinessKey AS 'QstExternalBusinessKey', fa.AnswerText as 'AnsText', fa.AnswerText as 'AnsValue',
'QuestionCategoryLabel'= isnull(fdqc.QuestionCategoryLabel, 'Not Specified')
from DW_FactAnswerText AS fa
	LEFT OUTER JOIN DW_DimQuestionAnswer AS qa ON fa.QuestionAnsID = qa.QuestionAnsID
		LEFT OUTER JOIN (
			select fqac.QuestionAnsID, 'QuestionCategoryID' = max(fqac.QuestionCategoryID), 'QuestionCategoryLabel' = max(qc.QuestionCategoryLabel)
			from DW_FactQuestionAnswerCategories fqac
			join DW_DimQuestionCategory qc on (fqac.QuestionCategoryID = qc.QuestionCategoryID)
			group by fqac.QuestionAnsID
			having count(*) = 1
		) AS fdqc ON qa.QuestionAnsID = fdqc.QuestionAnsID
WHERE (fa.SurveyID = @SurveyId) 
	AND (-1 in (@QuestionCategoryIds) or fdqc.QuestionCategoryID IN (@QuestionCategoryIds))


-- 3. insert the answers related to the SECONDARY categories for responses that are ONLY included in the primary answer set
insert into @secondaryAnswers
select fa.RspId, fa.SurveyID, fa.GroupID, qa.QuestionAnsID, qa.QuestionText, qa.QuestionReportAbbr, qa.QuestionAnsDataType, qa.AllowsMultipleAnswers, qa.AnsLabel,
qa.QuestionPageNumber, qa.QuestionDisplayOrder, qa.QuestionNumber, qa.DisplayOrder,
qa.ExternalBusinessKey AS 'QstExternalBusinessKey', fa.AnsText, qa.AnsValue,
'QuestionCategoryLabel'= isnull(fdqc.QuestionCategoryLabel, 'Not Specified')
from DW_FactAnswer AS fa
	LEFT OUTER JOIN DW_DimQuestionAnswer AS qa ON fa.QuestionAnsID = qa.QuestionAnsID
		LEFT OUTER JOIN (
			select fqac.QuestionAnsID, 'QuestionCategoryID' = max(fqac.QuestionCategoryID), 'QuestionCategoryLabel' = max(qc.QuestionCategoryLabel)
			from DW_FactQuestionAnswerCategories fqac
			join DW_DimQuestionCategory qc on (fqac.QuestionCategoryID = qc.QuestionCategoryID)
			group by fqac.QuestionAnsID
			having count(*) = 1
		) AS fdqc ON qa.QuestionAnsID = fdqc.QuestionAnsID
WHERE (fa.RspID in (select distinct RspID from @primaryAnswers))
	AND (fa.SurveyID = @SurveyId) 
	AND (-1 in (@SecondaryQuestionCategoryIds) or fdqc.QuestionCategoryID IN (@SecondaryQuestionCategoryIds))

insert into @secondaryAnswers
select fa.RspId, fa.SurveyID, fa.GroupID, qa.QuestionAnsID, qa.QuestionText, qa.QuestionReportAbbr, qa.QuestionAnsDataType, qa.AllowsMultipleAnswers, qa.AnsLabel,
qa.QuestionPageNumber, qa.QuestionDisplayOrder, qa.QuestionNumber, qa.DisplayOrder,
qa.ExternalBusinessKey AS 'QstExternalBusinessKey', fa.AnswerText as 'AnsText', fa.AnswerText as 'AnsValue',
'QuestionCategoryLabel'= isnull(fdqc.QuestionCategoryLabel, 'Not Specified')
from DW_FactAnswerText AS fa
	LEFT OUTER JOIN DW_DimQuestionAnswer AS qa ON fa.QuestionAnsID = qa.QuestionAnsID
		LEFT OUTER JOIN (
			select fqac.QuestionAnsID, 'QuestionCategoryID' = max(fqac.QuestionCategoryID), 'QuestionCategoryLabel' = max(qc.QuestionCategoryLabel)
			from DW_FactQuestionAnswerCategories fqac
			join DW_DimQuestionCategory qc on (fqac.QuestionCategoryID = qc.QuestionCategoryID)
			group by fqac.QuestionAnsID
			having count(*) = 1
		) AS fdqc ON qa.QuestionAnsID = fdqc.QuestionAnsID
WHERE (fa.RspID in (select distinct RspID from @primaryAnswers))
	AND (fa.SurveyID = @SurveyId) 
	AND (-1 in (@SecondaryQuestionCategoryIds) or fdqc.QuestionCategoryID IN (@SecondaryQuestionCategoryIds))

insert into @primaryAnswers select * from @secondaryAnswers


-- 4. return the UNION of the primary and secondary answers PLUS Response data
select r.*, 'SurveyTitle' = cast(year(s.StartDate) as varchar) + ' - ' + s.SurveyTitle, rs.StatusName, g.GroupCode, g.GroupName, d.*
from @primaryAnswers d
	join DW_DimResponse r on (d.RspId = r.RspID)
		join DW_DimResponseStatus rs on (r.StatusID = rs.StatusID)
	join DW_DimSurvey s on (d.SurveyId = s.SurveyID)
	left join DW_DimGroup g on (d.GroupId = g.GroupID)
where r.StatusId in (@RspStatusIds)
	and (-1 in (@GroupIds) or g.GroupID in (
		select distinct c.GroupID
		from DW_DimGroup c
			join DW_DimGroup p on (c.Path LIKE '%' + p.Path + '%')
		where c.IsArchived=0 and p.GroupID in (@GroupIds))
	)</CommandText></Query><Fields><Field Name="RspID"><DataField>RspID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="StatusID"><DataField>StatusID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Email"><DataField>Email</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Language"><DataField>Language</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="StartDate"><DataField>StartDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="LastActivityDate"><DataField>LastActivityDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="CustomValue1"><DataField>CustomValue1</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="ExternalBusinessKey"><DataField>ExternalBusinessKey</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CustomValue2"><DataField>CustomValue2</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CustomValue3"><DataField>CustomValue3</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CustomValue4"><DataField>CustomValue4</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesi